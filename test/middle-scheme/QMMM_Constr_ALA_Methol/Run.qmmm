#!/bin/sh

sander="../../../bin/msander"

if [ ! -z "$DO_PARALLEL" ]; then
   sander=$sander.MPI
fi

output=qmmm.out

echo "Testing MD with qmmm"
$DO_PARALLEL  $sander -O -i qmmm.in -p ala.top -c ala.crd -o $output \
    < /dev/null || { echo "  $0:  Program error"; exit 1; }

../../dacdif -r 1.e-6 $output.save $output
/bin/rm -f restrt mdinfo

exit 0 
