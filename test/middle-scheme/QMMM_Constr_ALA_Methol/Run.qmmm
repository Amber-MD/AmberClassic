#!/bin/csh

set sander = "../../../bin/msander"

  echo "Testing MD with qmmm"
  $sander -O \
          -i qmmm.in \
          -p ala.top \
          -c ala.crd \
          -o qmmm.out \
          -r qmmm.rst \
          -x qmmm.mdcrd \
          -inf qmmm.info || goto error
  ../../dacdif qmmm.out.save qmmm.out
  /bin/rm -f qmmm.info qmmm.rst

exit 0 

error:
echo "  ${0}:  Program error"
exit(1)
