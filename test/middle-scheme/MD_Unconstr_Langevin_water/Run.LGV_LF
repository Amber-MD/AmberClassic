#!/bin/csh -f

set sander = "../../../bin/msander"

  echo "Testing MD with langevin dynamics"
  /bin/rm -f md_LGV.out
  $sander -O \
          -i md_LGV.in \
          -p qspcfw216.top \
          -c nvt.rst \
          -o md_LGV.out \
          -r lgv.rst \
          -x lgv.crd \
          -inf lgv.info \
    || goto error
  ../../dacdif md_LGV.out.save md_LGV.out
  /bin/rm -f lgv.crd lgv.info lgv.rst

exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
