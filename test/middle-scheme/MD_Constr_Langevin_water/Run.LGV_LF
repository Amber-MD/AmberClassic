#!/bin/csh -f
#TEST-PROGRAM sander
#TEST-DESCRIP TO_BE_DEtermined
#TEST-PURPOSE regression, basic
#TEST-STATE   undocumented

set sander = "../../../bin/msander"

  echo "Testing MD with langevin dynamics"
  /bin/rm -f md_LGV.out
  $sander -O \
          -i md_LGV.in \
          -p qspcfw216.top \
          -c nvt.rst \
          -o md_LGV.out \
          -r nvt.rst.1 \
          -x lgv.crd \
          -inf lgv.info \
    || goto error
  ../../dacdif md_LGV.out.save md_LGV.out
  /bin/rm -f lgv.crd lgv.info nvt.rst.1

exit(0)

error:
echo "  ${0}:  Program error"
exit(1)
