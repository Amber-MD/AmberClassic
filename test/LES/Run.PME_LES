EOF

$DO_PARALLEL $sander  -O -i md.in -o md.LES.out \
    -p LES.prmtop.save -c md.LES.x \
    < /dev/null || { echo "  $0:  Program error"; exit 1; }

../dacdif md.LES.out.save  md.LES.out

/bin/rm -f mdinfo md.in restrt
exit 0 







